generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider          = "postgresql"
  url               = env("DATABASE_URL")
  shadowDatabaseUrl = env("SHADOW_DATABASE_URL")
}

model User {
  sid        String @id @unique @default(uuid())
  gender     GENDER
  name       Json
  fullname   String
  location   Json
  email      String @unique
  login      Json
  dob        Json
  registered Json
  phone      String
  cell       String
  id         Json
  picture    Json
  nat        String
}

enum GENDER {
  male
  female
  other
}
